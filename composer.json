{
  "name": "jaulz/laravel-navigare",
  "type": "library",
  "description": "The Laravel adapter for Navigare.",
  "keywords": [
    "laravel",
    "navigare"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hundeloh",
      "email": "github@julian.fyi",
      "homepage": "https://hundeloh-consulting.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jaulz\\Navigare\\": "src"
    },
    "files": [
      "./helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jaulz\\Navigare\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2|~8.0.0|~8.1.0",
    "ext-json": "*",
    "laravel/framework": "^9.36"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^7.10",
    "mockery/mockery": "^1.3.3",
    "phpunit/phpunit": "^8.0|^9.5.8",
    "pestphp/pest": "^1.22",
    "spatie/pest-plugin-snapshots": "^1.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jaulz\\Navigare\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
